.Map-DateAxis{
	width: 551px;
	height: 61px;
	rgba(51, 99, 183, 0.7) url(../images/Minute-ico1.png) no-repeat 58px 40px
	bottom: 20px;
    right: 20px;
    position: absolute;
    border-radius: 4px;
}
.Map-DateAxis .Date{
	background: url(../images/Date.png) no-repeat 7px center;
	width: 57px;
	height: 61px;
	color: #fff;
    font-size: 12px;
    text-align: center;
    padding-top: 22px;
    padding-left: 6px;
    font-weight: bold;
    line-height: 14px;
    box-sizing: border-box;
    float: left;
}
.timeAxis{
	height: 30px;
    float: left;
    width: 400px;
    margin-top: 10px;
}
.timeAxis-top{
	width: 100%;
    height: 30px;
}
.timeAxis-top .bn-arrows{
	float: left;
	margin-top: 5px;
}
.timeAxis-top .bn-arrows span{
	display: block;
    width: 20px;
    cursor: pointer;
    height: 20px;
}
.timeAxis-top .bn-arrows .bn-arrows-left{
	background: url(../images/left.png) no-repeat 0;
	margin-left: 12px;
}
.timeAxis-top .bn-arrows .bn-arrows-right{
	background: url(../images/right.png) no-repeat 0;
}
.timeAxis-top ul{
	float: left;
	margin-top: 4px;
    padding: 0px;
    list-style: none;
}
.timeAxis-top ul li{
    overflow: hidden;
}
.timeAxis-top ul li div{
	color: #fff;
    float: left;
    width: 26px;
    height: 22px;
    line-height: 22px;
    text-align: center;
    font-size: 12px;
    margin-right: 2.4px;
    cursor: pointer;
}
.timeAxis-top ul li div.active{
	width: 26px;
	height: 22px;
	line-height: 22px;
	background: #1c4880;
	border-radius: 4px;
}
.timeAxis-bottom{
	width: 400px;
	float: left;
	margin-top: 9px;
    margin-left: 15px;
    display: none;
}
.left-time{
	float: left;
	background: url(../images/left-time.png) no-repeat;
	width: 17px;
	height: 17px;
}
.ProgressBar{
	float: left;
    background: #1c4880;
    height: 4px;
    width: 315px;
    border-radius: 6px;
    margin-top: 7px;
    margin-left: 10px;
    margin-right: 10px;
    position: relative;
}
.right-time{
	float: left;
	background: url(../images/right-time.png) no-repeat;
	width: 17px;
	height: 17px;
}
.ProgressBar-add{
	animation-duration: 10s;
    animation-name: width;
    background-image: linear-gradient(to right, #16c1e5, #16c1e5);
    background-size: 24em 0.25em;
    height: 100%;
    border-radius: 6px;
    width: 50%;
    box-shadow: 0px 0px 10px 0.5px #16c1e5;
}
.ProgressBar-add1{
	animation-duration: 10s;
    animation-name: width;
    background-image: linear-gradient(to right, #16c1e5, #16c1e5);
    background-size: 24em 0.25em;
    height: 10px;
    border-radius: 10px;
    width: 10px;    
    position: absolute;
    background: #16c1e5;
    top: -3px;
    left: 49%;
    cursor: pointer;
}


.right-play{
	position: relative;
	float: right;
    height: 61px;
    width: 92px;
}
.play{
	background: url(../images/play.png) no-repeat 9px center #1c4880;
    position: absolute;
    width: 32px;
    height: 32px;
    border-radius: 32px;
    top: 15px;
    cursor: pointer;
}
.stop{
	background: url(../images/stop.png) no-repeat 9px center #1c4880;
    position: absolute;
    width: 32px;
    height: 32px;
    border-radius: 32px;
    top: 15px;
    cursor: pointer;
}
.suspend{
	background: url(../images/suspend.png) no-repeat center rgba(28, 72, 128, 0.50);
    position: absolute;
    width: 27px;
    height: 27px;
    border-radius: 27px;
    top: 17px;
    left: 30px;
    cursor: pointer;
}
.locking{
	background: url(../images/locking.png) no-repeat center;
	width: 16px;
	height: 16px;
	float: right;
    margin-top: 20px;
    margin-right: 12px;
    cursor: pointer;
}
.unlock{
	background: url(../images/Unlock.png) no-repeat center;
	width: 16px;
	height: 16px;
	float: right;
    margin-top: 20px;
    margin-right: 12px;
    cursor: pointer;
}